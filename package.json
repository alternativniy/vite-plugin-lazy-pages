{
  "name": "vite-plugin-lazy-pages",
  "private": false,
  "version": "0.2.2",
  "description": "React file based routing for Vite",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/alternativniy/vite-plugin-lazy-pages",
  "bugs": "https://github.com/alternativniy/vite-plugin-lazy-pages/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/alternativniy/vite-plugin-lazy-pages"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "react",
    "react-router",
    "file-system-based",
    "routing"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "files": [
    "dist",
    "client.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && unbuild",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react-router-dom": "^6.9.0",
    "vite": ">=2"
  },
  "devDependencies": {
    "@types/node": "^20.11.13",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.21.3",
    "react-router-dom": "^6.21.3",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^5.0.8"
  }
}
